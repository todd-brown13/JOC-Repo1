# Calculate Average of 20 numbers.py
# Todd Brown
# 4/1//24
# This program will prompt for 10 decimal numbers, then print the average of those numbers.

def main():
    max_numbers = 5
    num = 0
    num_List = []
    total = 0
    for x in range(max_numbers):
        num = float(input( "Enter a decimal number "))
        num_List.append( num)
        total += num
    avg = total/max_numbers
    print("The Average of list", (num_List), "=", avg)

main()
